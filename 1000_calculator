int plus(int*num1, int * num2){
    int j [2];
    int a;
    result[1000];
    for(int i=999; i--;){
        a = num1[i] + num2[i];
        j[1] = a /10;
        j[0]=a%10;
        result[i] = j[1];
        num1[i-1]=j[0];
        
    }
}

int minus(int*num1, int * num2){
    for(int i=999; i>=0; i--){
        num1[i]-num2[i];
    }
}
